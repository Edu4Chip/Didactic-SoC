	<ipxact:memoryMaps>
		<ipxact:memoryMap>
			<ipxact:name>apb_spi_master</ipxact:name>
			<ipxact:addressBlock misalignmentAllowed="true">
				<ipxact:name>registers</ipxact:name>
				<ipxact:baseAddress>'h0</ipxact:baseAddress>
				<ipxact:range>44</ipxact:range>
				<ipxact:width>32</ipxact:width>
				<ipxact:register>
					<ipxact:name>STATUS</ipxact:name>
					<ipxact:description>Status Register</ipxact:description>
					<ipxact:addressOffset>'h0</ipxact:addressOffset>
					<ipxact:size>32</ipxact:size>
					<ipxact:field>
						<ipxact:name>QWR</ipxact:name>
						<ipxact:description>Assert this for quad write mode</ipxact:description>
						<ipxact:bitOffset>3</ipxact:bitOffset>
						<ipxact:bitWidth>1</ipxact:bitWidth>
					</ipxact:field>
					<ipxact:field>
						<ipxact:name>RD</ipxact:name>
						<ipxact:description>Assert this for read operation</ipxact:description>
						<ipxact:bitOffset>0</ipxact:bitOffset>
						<ipxact:bitWidth>1</ipxact:bitWidth>
						<ipxact:resets>
							<ipxact:reset>
								<ipxact:value>'b1</ipxact:value>
							</ipxact:reset>
						</ipxact:resets>
					</ipxact:field>
					<ipxact:field>
						<ipxact:name>WR</ipxact:name>
						<ipxact:description>Assert this for write operation</ipxact:description>
						<ipxact:bitOffset>1</ipxact:bitOffset>
						<ipxact:bitWidth>1</ipxact:bitWidth>
					</ipxact:field>
					<ipxact:field>
						<ipxact:name>QRD</ipxact:name>
						<ipxact:description>Assert this for quad read mode</ipxact:description>
						<ipxact:bitOffset>2</ipxact:bitOffset>
						<ipxact:bitWidth>1</ipxact:bitWidth>
					</ipxact:field>
					<ipxact:field>
						<ipxact:name>SRST</ipxact:name>
						<ipxact:description>Assert this to clear the rx and tx FIFOs</ipxact:description>
						<ipxact:bitOffset>4</ipxact:bitOffset>
						<ipxact:bitWidth>1</ipxact:bitWidth>
					</ipxact:field>
					<ipxact:field>
						<ipxact:name>CS</ipxact:name>
						<ipxact:description>Each one of these bits corresponds to one of the spi lines. Enable any of them to enable the cs on the corresponding spi line.</ipxact:description>
						<ipxact:bitOffset>8</ipxact:bitOffset>
						<ipxact:bitWidth>4</ipxact:bitWidth>
					</ipxact:field>
				</ipxact:register>
				<ipxact:register>
					<ipxact:name>CLKDIV</ipxact:name>
					<ipxact:description>Clock Divider</ipxact:description>
					<ipxact:addressOffset>'h4</ipxact:addressOffset>
					<ipxact:size>32</ipxact:size>
					<ipxact:field>
						<ipxact:name>CLKDIV</ipxact:name>
						<ipxact:bitOffset>0</ipxact:bitOffset>
						<ipxact:bitWidth>8</ipxact:bitWidth>
					</ipxact:field>
				</ipxact:register>
				<ipxact:register>
					<ipxact:name>SPICMD</ipxact:name>
					<ipxact:description>SPI Command</ipxact:description>
					<ipxact:addressOffset>'h8</ipxact:addressOffset>
					<ipxact:size>32</ipxact:size>
					<ipxact:field>
						<ipxact:name>SPICMD</ipxact:name>
						<ipxact:description>Insert the command to be transmitted</ipxact:description>
						<ipxact:bitOffset>0</ipxact:bitOffset>
						<ipxact:bitWidth>32</ipxact:bitWidth>
					</ipxact:field>
				</ipxact:register>
				<ipxact:register>
					<ipxact:name>SPIADR</ipxact:name>
					<ipxact:description>SPI Address</ipxact:description>
					<ipxact:addressOffset>'h0C</ipxact:addressOffset>
					<ipxact:size>32</ipxact:size>
					<ipxact:field>
						<ipxact:name>SPIADR</ipxact:name>
						<ipxact:description>Insert the address to be transmitted</ipxact:description>
						<ipxact:bitOffset>0</ipxact:bitOffset>
						<ipxact:bitWidth>32</ipxact:bitWidth>
					</ipxact:field>
				</ipxact:register>
				<ipxact:register>
					<ipxact:name>SPILEN</ipxact:name>
					<ipxact:description>SPI Transfer Length</ipxact:description>
					<ipxact:addressOffset>'h10</ipxact:addressOffset>
					<ipxact:size>32</ipxact:size>
					<ipxact:field>
						<ipxact:name>CMDLEN</ipxact:name>
						<ipxact:description>The number of bits of the SPI command that should be sent.</ipxact:description>
						<ipxact:bitOffset>0</ipxact:bitOffset>
						<ipxact:bitWidth>6</ipxact:bitWidth>
					</ipxact:field>
					<ipxact:field>
						<ipxact:name>ADDRLEN</ipxact:name>
						<ipxact:description>The number of bits of the SPI address that should be sent.
</ipxact:description>
						<ipxact:bitOffset>8</ipxact:bitOffset>
						<ipxact:bitWidth>6</ipxact:bitWidth>
					</ipxact:field>
					<ipxact:field>
						<ipxact:name>DATALEN</ipxact:name>
						<ipxact:description>The number of data bits to be read or written.</ipxact:description>
						<ipxact:bitOffset>16</ipxact:bitOffset>
						<ipxact:bitWidth>16</ipxact:bitWidth>
					</ipxact:field>
				</ipxact:register>
				<ipxact:register>
					<ipxact:name>SPIDUM</ipxact:name>
					<ipxact:description>SPI Dummy Cycles</ipxact:description>
					<ipxact:addressOffset>'h14</ipxact:addressOffset>
					<ipxact:size>32</ipxact:size>
					<ipxact:field>
						<ipxact:name>DUMMYRD</ipxact:name>
						<ipxact:description>Dummy cycles (nothing being written or read) between sending the SPI command +
SPI address and reading the data.
</ipxact:description>
						<ipxact:bitOffset>0</ipxact:bitOffset>
						<ipxact:bitWidth>16</ipxact:bitWidth>
					</ipxact:field>
					<ipxact:field>
						<ipxact:name>DUMMYWR</ipxact:name>
						<ipxact:description>Dummy cycles (nothing being written or read) between sending the SPI command +
SPI address and writing the data.
</ipxact:description>
						<ipxact:bitOffset>16</ipxact:bitOffset>
						<ipxact:bitWidth>16</ipxact:bitWidth>
					</ipxact:field>
				</ipxact:register>
				<ipxact:register>
					<ipxact:name>TXFIFO</ipxact:name>
					<ipxact:description>SPI Transmit FIFO</ipxact:description>
					<ipxact:addressOffset>'h18</ipxact:addressOffset>
					<ipxact:size>32</ipxact:size>
					<ipxact:field>
